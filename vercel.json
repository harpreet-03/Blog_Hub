{
  "version": 2,
  "builds": [
    {
      "src": "django_web_app/django_web_app/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "django_web_app/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/django_web_app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "django_web_app/django_web_app/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "django_web_app.production_settings"
  }
}
